@{
    Layout = null;
}
<html ng-app="WebAPI">
<head>
    <title>Taxi Service</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    @*<link rel="stylesheet" href="../../MyCSSs/StyleSheet1.css" type="text/css" />*@
    <link rel="shortcut icon" href="../favicon.ico">

    <link rel="stylesheet" href="https://openlayers.org/en/v4.6.5/css/ol.css" type="text/css">
    <style>
        .map {
            height: 400px;
            width: 100%;
        }
    </style>
    

    @*<link rel="stylesheet" href="../../css/demo.css" type="text/css" />
        <link rel="stylesheet" href="../../css/style.css" type="text/css" />
        <link rel="stylesheet" href="../../css/animate-custom.css" type="text/css" />*@

    <link rel="stylesheet" href="../../Content/bootstrap.min.css" type="text/css" />
    <link href="https://fonts.googleapis.com/css?family=Montserrat:300,400,500,600,700" rel="stylesheet">
    @*ng-controller="MainController"*@
</head>
<body>
    <div class="container-fluid">
        <nav class="navbar navbar-default navbar-fixed-top" ng-controller="MainController">
            <div class="container-fluid">

                <div class="navbar-header">
                    <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>

                </div>

                <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                    <ul class="nav navbar-nav">
                        <li ng-show="loggedin" ng-class="checkActive('/MyHome')"><a href="#!/"><i class="ion ion-android-home"></i> Home</a></li>
                        <li ng-show="loggedin" ng-class="checkActive('/Edit')"><a href="#!/Edit/{{user.username}}"><i class="ion ion-person"></i>Edit Profile</a></li>
                    </ul>
                    <ul class="nav navbar-nav navbar-right">
                        <li ng-show="!loggedin" ng-class="checkActive('/Login')"><a href="#!/Login">Login</a></li>
                        <li ng-show="!loggedin" ng-class="checkActive('/Register')"><a href="#!/Register">Register</a></li>
                        <li ng-show="loggedin" ng-class="checkActive('/Profile')"><a href="#!/Profile/{{user.username}}"><i class="ion ion-person"></i> {{user.nameSurname}}</a></li>
                        <li ng-show="loggedin"><a href ng-click="Logout()"><i class="ion ion-log-out"></i> Logout</a></li>

                    </ul>
                </div><!-- /.navbar-collapse    class="glyphicon glyphicon-user"   class="glyphicon glyphicon-log-in" -->
            </div><!-- /.container-fluid -->
        </nav>
        <div ng-view></div>
    </div>

    <script src="../../Scripts/jquery-1.10.2.min.js"></script>
    <script src="../../Scripts/bootstrap.min.js"></script>
    <script src="../../Scripts/angular.min.js"></script>
    <script src="../../Scripts/angular-route.min.js"></script>


    <script src="../../Factories/PageHelp.js"></script>
    <script src="../../JSControllers/MainController.js"></script>
    <script src="../../JSControllers/MyHomeController.js"></script>
   
    <script src="../../JSControllers/Filter2Controller.js"></script>
    
    <script src="../../JSControllers/FilterController.js"></script>
    



    <script src="https://openlayers.org/en/v4.6.5/build/ol.js" type="text/javascript"></script>
    <script src="../../JSControllers/ProfileController.js"></script>
    <script src="../../JSControllers/JSRegILogController.js"></script>
    <script src="../../Factories/RegILogFactory.js"></script>
    <script src="../../Factories/ProfCont.js"></script>
    <script src="../../Factories/MyDirective.js"></script>

</body>
</html>